{
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "start-server": "bundle exec rackup -p 9292",
    "cypress:open": "cypress open",
    "cypress:ci": "start-server-and-test 'RACK_ENV=test bundle exec rackup -p 9292' http-get://localhost:9292/health 'cypress run'",
    "start": "parcel serve ./client/src/index.tsx",
    "build": "parcel build ./client/src/index.tsx --dist-dir ./public --log-level=error",
    "typecheck": "tsc --noEmit -p client",
    "clean": "rm -rf client/dist && rm -rf .parcel-cache",
    "test": "jest",
    "lint": "eslint 'client/src/**/*.{js,jsx,ts,tsx}'",
    "lintfix": "eslint 'client/src/**/*.{js,jsx,ts,tsx}' --fix",
    "build:theme": "lessc --js ./client/src/index.less ./client/src/antd.css"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@parcel/transformer-svg-react": "^2.0.0-nightly.1739",
    "@svgr/parcel-plugin-svgr": "^5.4.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.6",
    "@types/enzyme": "^3.10.5",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "autoprefixer": "^9.8.6",
    "babel-plugin-import": "^1.13.0",
    "browserslist": "^4.13.0",
    "cypress": "^4.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.1",
    "jest-fetch-mock": "^3.0.3",
    "jsonwebtoken": "^8.5.1",
    "lessc": "^1.0.2",
    "parcel": "^2.0.0-nightly.353",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-modules": "^3.2.0",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.2",
    "typescript": "^3.9.7",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.4",
    "@ant-design/icons": "^4.2.1",
    "@apollo/client": "^3.1.1",
    "@enterprise-oss/osso": "^0.0.5-alpha.33",
    "antd": "^4.5.1",
    "fast-xml-parser": "^3.17.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "use-debounce": "^3.4.3"
  },
  "browserslist": {
    "development": [
      "last 1 Chrome version"
    ],
    "production": [
      "defaults"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yalc check"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(png|svg)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "~(.*)$": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.yalc/*",
      "<rootDir>/node_modules/*",
      "<rootDir>/cypress/*"
    ],
    "setupFiles": [
      "<rootDir>/client/jest.setup.js"
    ]
  },
  "engines": {
    "node": ">= 14.4.0"
  }
}
