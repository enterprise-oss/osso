{
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "start-server": "bundle exec rackup -p 9292",
    "cypress:open": "cypress open",
    "cypress:ci": "start-server-and-test 'bundle exec rackup -p 9292' http-get://localhost:9292/health 'cypress run'",
    "start": "yalc add @enterprise-oss/osso && parcel serve ./client/src/index.tsx",
    "build": "parcel build ./client/src/index.tsx --log-level warn --dist-dir ./public",
    "checktypes": "tsc -p client",
    "clean": "rm -rf client/dist && rm -rf .parcel-cache",
    "test": "jest",
    "lint": "eslint 'client/src/**/*.{js,jsx,ts,tsx}'",
    "lintfix": "eslint 'client/src/**/*.{js,jsx,ts,tsx}' --fix",
    "build:theme": "lessc --js ./client/src/index.less ./src/antd.css"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@svgr/parcel-plugin-svgr": "^5.4.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/enzyme": "^3.10.5",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-plugin-import": "^1.13.0",
    "browserslist": "^4.12.2",
    "cypress": "^4.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.4.0",
    "jest-fetch-mock": "^3.0.3",
    "jsonwebtoken": "^8.5.1",
    "parcel": "^2.0.0-nightly.329",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0",
    "typescript": "^3.8.0"
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.0",
    "@ant-design/icons": "^4.2.1",
    "@apollo/client": "^3.0.0-rc.10",
    "@enterprise-oss/osso": "^0.0.5-alpha.27",
    "antd": "^4.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "browserslist": {
    "development": [
      "last 1 Chrome version"
    ],
    "production": [
      "defaults"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yalc check"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(png|svg)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.yalc/*",
      "<rootDir>/node_modules/*",
      "<rootDir>/cypress/*"
    ],
    "setupFiles": [
      "<rootDir>/client/jest.setup.js"
    ]
  }
}
